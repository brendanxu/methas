{
  "name": "southpole-official",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "build:standalone": "BUILD_STANDALONE=true next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "setup": "node scripts/generate-brand-assets.js && npm install",
    "optimize-images": "node scripts/optimize-images.js",
    "image-utils": "node scripts/image-utils.js",
    "generate-blur": "node scripts/image-utils.js blur",
    "image-metadata": "node scripts/image-utils.js metadata",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "bundle-report": "npx webpack-bundle-analyzer .next/analyze/bundle/client.html",
    "bundle-analyze": "node scripts/analyze-bundle.js",
    "perf:audit": "lhci autorun",
    "perf:collect": "lhci collect",
    "perf:assert": "lhci assert",
    "perf:upload": "lhci upload",
    "perf:server": "lhci server",
    "perf:ci": "npm run build && lhci autorun",
    "test:performance": "npm run perf:ci",
    "monitor:lighthouse": "lhci autorun --config=lighthouserc.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "npm run type-check && npm run lint && npm run test",
    "deploy:vercel": "vercel --prod",
    "clean": "rm -rf .next && rm -rf out && rm -rf dist",
    "postinstall": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@auth/prisma-adapter": "^2.10.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.11.1",
    "@sendgrid/mail": "^8.1.5",
    "@sentry/nextjs": "^9.38.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcryptjs": "^3.0.0",
    "@types/chalk": "^0.4.31",
    "@types/lodash-es": "^4.17.12",
    "@types/multer": "^2.0.0",
    "@types/nprogress": "^0.2.3",
    "antd": "^5.26.4",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.3.5",
    "glob": "^11.0.3",
    "global": "^4.4.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lodash-es": "^4.17.21",
    "multer": "^2.0.1",
    "next": "15.3.5",
    "next-auth": "^4.24.11",
    "next-i18next": "^15.4.2",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.49",
    "prisma": "^6.11.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.60.0",
    "react-i18next": "^15.6.0",
    "react-intersection-observer": "^9.16.0",
    "react-quill": "^2.0.0",
    "tailwind-merge": "^2.5.4",
    "web-vitals": "^5.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.3.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "chalk": "^5.4.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "lighthouse": "^12.8.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sharp": "^0.33.1",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
